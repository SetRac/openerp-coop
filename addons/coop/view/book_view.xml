<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!-- SEARCH VIEW -->
        <record id="view_coop_book_filter" model="ir.ui.view">
            <field name="name">coop.book.select</field>
            <field name="model">coop.book</field>
            <field name="arch" type="xml">
                <search string="book" version="7.0">
                    <group string="By Attribute">
                    </group>
                    <group string="By object">
                    </group>
                </search>
            </field>
        </record>

        <!-- FORMVIEW -->
        <record id="view_coop_book_form" model="ir.ui.view">
            <field name="name">coop.book.form</field>
            <field name="model">coop.book</field>
            <field name="arch" type="xml">
                <form string="book" version="7.0">
                    <header>
                    </header>
                    <sheet string="book">
                     <group>
                        <field name="name"
                            />
                    <field name="sequence_id"
                            />
                     </group>
                    </sheet>

                </form>
            </field>
        </record>

        <!-- TREEVIEW -->

        <record id="view_coop_book_tree" model="ir.ui.view">
            <field name="name">coop.book.tree</field>
            <field name="model">coop.book</field>
            <field name="arch" type="xml">
                <tree string="book"
                    >
                    <field name="name"
                        />
                </tree>
            </field>
        </record>

    </data>
</openerp>
